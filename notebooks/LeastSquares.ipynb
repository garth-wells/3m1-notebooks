      "\n",
      "    # Uncomment the line below to add noise\n",
      "    #y_p = y_p + 2.0e-2*(np.random.rand(len(y_p)) - 0.5)\n",
      "    \n",
      "    # Create an array of 200 equally spaced points on [-pi, pi]\n",
      "    x_fit = np.linspace(-3*np.pi, 3*np.pi, 200) \n",
      "    \n",
      "    # Compute least squares fit\n",
      "    c_ls = np.polyfit(x_p, y_p, degree)\n",
      "    p_ls = np.poly1d(c_ls)\n",
      "    y_ls = p_ls(x_fit)\n",
      "    \n",
      "    # Compute polynomial interpolation\n",
      "    A = np.vander(x_p, num_data_points)\n",
      "    c = np.linalg.solve(A, y_p)\n",
      "    p = np.poly1d(c)\n",
      "    y_int = p(x_fit)\n",
      "\n",
      "    # Plot\n",
      "    plt.grid()\n",
      "    #plt.xlim(-3.5,3.5)\n",
